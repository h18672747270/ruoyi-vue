#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/vite@7.0.1_@types+node@24.0_9b354ea597eb9c722f16120b64f47d88/node_modules/vite/bin/node_modules:/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/vite@7.0.1_@types+node@24.0_9b354ea597eb9c722f16120b64f47d88/node_modules/vite/node_modules:/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/vite@7.0.1_@types+node@24.0_9b354ea597eb9c722f16120b64f47d88/node_modules:/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/vite@7.0.1_@types+node@24.0_9b354ea597eb9c722f16120b64f47d88/node_modules/vite/bin/node_modules:/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/vite@7.0.1_@types+node@24.0_9b354ea597eb9c722f16120b64f47d88/node_modules/vite/node_modules:/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/vite@7.0.1_@types+node@24.0_9b354ea597eb9c722f16120b64f47d88/node_modules:/mnt/d/project/ruoyi-plus-soybean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
